	"authors": [
		"Arcadesmasher"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/pickup-filter/",
		"sources": "https://github.com/arcade-smasher/item-filter/",
		"issues": "https://github.com/arcade-smasher/item-filter/issues/"
	},
	"license": "MIT",
	"icon": "assets/itemfilter/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.itemfilter.ItemFilterMod"
		],
		"mixins": [
			"ItemEntityMixin"
		]
	},
	"mixins": [
		"itemfilter.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": ">=1.20 <1.20.2",
		"java": ">=17",
