	"authors": [
		"Arcadesmasher"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/pickup-filter/",
		"sources": "https://github.com/arcade-smasher/item-blacklist/",
		"issues": "https://github.com/arcade-smasher/item-blacklist/issues/"
	},
	"license": "MIT",
	"icon": "assets/blacklist/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.blacklist.ItemBlacklistMod"
		],
		"mixins": [
			"ItemEntityMixin"
		]
	},
	"mixins": [
		"blacklist.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.20.1",
